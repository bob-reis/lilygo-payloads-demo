REM Teste 2: Abrir prompt de comando
DELAY 2000
GUI R
DELAY 500
STRING cmd
ENTER
DELAY 1000

REM Teste de comando com barra invertida
STRING cd
RAW_HID 0x73
ENTER
DELAY 500

STRING dir
ENTER
DELAY 1000

STRING exit
ENTER

REM Finalizar indicando sucesso
DELAY 1000
ALT F4
DELAY 500
STRING n
ENTER